    'This method is called when this script task executes in the control flow.
    'Before returning from this method, set the value of Dts.TaskResult to indicate success or failure.
    'To open Help, press F1.

    Public Sub Main()

        'Integers
        Dim intCallingAppId As Integer = CInt(Dts.Variables("User::_numCallingAppId").Value)
        Dim intEntryUserId As Integer = CInt(Dts.Variables("User::_numEntryUserId").Value)

        'OLEDB objects
        Dim objOLEDBConnection As New System.Data.OleDb.OleDbConnection
        Dim objOLEDBCommand As New System.Data.OleDb.OleDbCommand

        'Strings
        Dim strSendEmailBody As String = Dts.Variables("User::_strSendEmailMessageSource").Value.ToString.Trim
        Dim strSendEmailTo As String = Dts.Variables("User::_strSendEmailToLine").Value.ToString.Trim
        Dim strSPStatus As String = "FAILED"
        Dim strSQL As String = "ssp_SendEmail"
        Dim strSQLServerDatabaseConnectString As String = Dts.Variables("_strSQLServerDatabaseConnectString").Value.ToString.Trim
        Dim strSendEmailSubject As String = Dts.Variables("User::_strSendEmailSubject").Value.ToString.Trim
        Dim strUseSendEmailTo As String = Dts.Variables("User::strUseSendEmailTo").Value.ToString.Trim

        Try
                With objOLEDBConnection
                    .ConnectionString = Dts.Variables("_strSQLServerDatabaseConnectString").Value.ToString.Trim
                    .Open()
                End With

                With objOLEDBCommand
                    .Connection = objOLEDBConnection
                    .CommandType = CommandType.StoredProcedure
                    .CommandText = strSQL
                    .Parameters.Add("@p_nCallingAppId", OleDb.OleDbType.Numeric).Value = intCallingAppId
                    .Parameters.Add("@p_nEntryUserId", OleDb.OleDbType.Numeric).Value = intCallingAppId
                    .Parameters.Add("@p_vcSendEmailTo", OleDb.OleDbType.VarChar, 4000).Value = strSendEmailTo
                    .Parameters.Add("@p_vcUseSendEmailTo", OleDb.OleDbType.VarChar, 1).Value = strUseSendEmailTo
                    .Parameters.Add("@p_vcSendEmailSubject", OleDb.OleDbType.VarChar, 255).Value = strSendEmailSubject
                    .Parameters.Add("@p_vcSendEmailBody", OleDb.OleDbType.VarChar, 4000).Value = strSendEmailBody
                    .Parameters.Add("@p_vcSPStatus", OleDb.OleDbType.VarChar, 4000).Direction = ParameterDirection.Output
                    .ExecuteNonQuery()
                End With

                strSPStatus = objOLEDBCommand.Parameters("@p_vcSPStatus").Value.ToString
                If strSPStatus.ToString.Trim.ToUpper = "SUCCESS" Then
                    'Do Nothing
                Else
                    Err.Raise(999, _
                                "SSIS: Send Email Package Failed", _
                                strSPStatus.ToString.Trim)
                End If

                objOLEDBCommand.Dispose()
                objOLEDBCommand = Nothing

                objOLEDBConnection.Close()
                objOLEDBConnection.Dispose()
                objOLEDBConnection = Nothing
        Catch
                Dts.TaskResult = ScriptResults.Failure
        End Try

    End Sub

